let dxlOperatorList  = '[-&|+<>=*/!~]'    " A list of symbols that we don't want to immediately precede the operator
let dxlOperatorList .= '\@<!'             " Negative look-behind (check that the preceding symbols aren't there)
let dxlOperatorList .= '\%('              " Beginning of a list of possible operators
let dxlOperatorList .=     '\('           " First option, the following symbols...
let dxlOperatorList .=        '[-&|+<>=]'
let dxlOperatorList .=     '\)'
let dxlOperatorList .=     '\1\?'         " Followed by (optionally) the exact same symbol, so -, --, =, ==, &, && etc
let dxlOperatorList .= '\|'               " Next option:
let dxlOperatorList .=     '->'           " Pointer dereference operator
let dxlOperatorList .= '\|'               " Next option:
let dxlOperatorList .=     '[-+*/%&^|!]=' " One of the listed symbols followed by an =, e.g. +=, -=, &= etc
let dxlOperatorList .= '\|'               " Next option:
let dxlOperatorList .=     '[*?,!~%]'     " Some simple single character operators
let dxlOperatorList .= '\|'               " Next option:
let dxlOperatorList .=     '\('           " One of the shift characters:
let dxlOperatorList .=         '[<>]'     
let dxlOperatorList .=     '\)'
let dxlOperatorList .=     '\2'           " Followed by another identical character, so << or >>...
let dxlOperatorList .=     '='            " Followed by =, so <<= or >>=.
let dxlOperatorList .= '\)'               " End of the long list of options
let dxlOperatorList .= '[-&|+<>=*/!~]'    " The list of symbols that we don't want to follow
let dxlOperatorList .= '\@!'              " Negative look-ahead (this and the \@<! prevent === etc from matching)

exe "syn match dxlOperator display '" . dxlOperatorList . "'"

syn match dxlOperator display ';'
hi link dxlOperator Operator

